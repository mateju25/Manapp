<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="239.0" prefWidth="885.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.base.MainPageController">
   <children>
      <TextField fx:id="textTime" layoutX="30.0" layoutY="88.0" promptText="Čas" />
      <ComboBox fx:id="comboDay" layoutX="30.0" layoutY="53.0" prefWidth="150.0" promptText="Deň" />
      <TextField fx:id="textLink" layoutX="30.0" layoutY="124.0" promptText="Link" />
      <Button fx:id="btnAdd" layoutX="89.0" layoutY="162.0" mnemonicParsing="false" onAction="#addLink" text="OK" />
      <TextField fx:id="textName" layoutX="30.0" layoutY="19.0" promptText="Názov" />
      <ListView fx:id="viewLinks" layoutX="200.0" layoutY="19.0" onMouseClicked="#loadItem" prefHeight="200.0" prefWidth="661.0" />
   </children>
</AnchorPane>
